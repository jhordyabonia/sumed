<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceBlock name="weltpixel.prefooter" remove="true"/>
        <referenceBlock name="weltpixel.footer" remove="true"/>


        <referenceContainer name="footer">

            <block name="footer-sections" template="WeltPixel_CustomFooter::footer-sections.phtml"/>
            <block name="bottom-bar" template="WeltPixel_CustomFooter::bottom-bar.phtml"/>

        </referenceContainer>

    </body>
</page>
