<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">sumed_envios_envios_form.envios_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Sumed\Envios\Block\Adminhtml\Envios\Edit\BackButton"/>
			<button name="delete" class="Sumed\Envios\Block\Adminhtml\Envios\Edit\DeleteButton"/>
			<button name="save" class="Sumed\Envios\Block\Adminhtml\Envios\Edit\SaveButton"/>
			<button name="save_and_continue" class="Sumed\Envios\Block\Adminhtml\Envios\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>sumed_envios_envios_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>sumed_envios_envios_form.envios_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="envios_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="envios_form_data_source" class="Sumed\Envios\Model\Envios\DataProvider">
			<settings>
				<requestFieldName>envios_id</requestFieldName>
				<primaryFieldName>envios_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="Codigo" formElement="textarea" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">envios</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Codigo</label>
				<dataScope>Codigo</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="Fecha_Incio" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">envios</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Fecha_Incio</label>
				<dataScope>Fecha_Incio</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="Dias" formElement="textarea" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">envios</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Dias</label>
				<dataScope>Dias</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="Frecuencia" formElement="textarea" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">envios</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Frecuencia</label>
				<dataScope>Frecuencia</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="Horas" formElement="textarea" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">envios</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Horas</label>
				<dataScope>Horas</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
